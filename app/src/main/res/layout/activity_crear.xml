<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Crear">

    <ImageView
            android:layout_width="227dp"
            android:layout_height="209dp" app:srcCompat="@drawable/p"
            android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="78dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="286dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="78dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/Nombre"
            android:ems="10"
            tools:layout_editor_absoluteY="250dp" tools:layout_editor_absoluteX="26dp" android:id="@+id/editText"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/Telefono"
            android:ems="10"
            tools:layout_editor_absoluteY="320dp" tools:layout_editor_absoluteX="26dp" android:id="@+id/editText2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/Email"
            android:ems="10"
            tools:layout_editor_absoluteY="384dp" tools:layout_editor_absoluteX="26dp" android:id="@+id/editText4"/>
    <Button
            android:text="@string/crear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="447dp"
            tools:layout_editor_absoluteX="68dp" android:id="@+id/button2"/>
    <Button
            android:text="@string/Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="447dp"
            tools:layout_editor_absoluteX="213dp" android:id="@+id/button3"/>
</android.support.constraint.ConstraintLayout>